<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mapper.AttendanceSettingMapper">
    <update id="updateAttendanceSetting" parameterType="AttendanceSetting">
        update attendancesetting set starttime=#{starttime},lateststarttime=#{lateststarttime},endtime=#{endtime}
    </update>
    <select id="selectAttendanceSetting" resultType="AttendanceSetting">
        select starttime,lateststarttime,endtime from attendancesetting
    </select>
</mapper>